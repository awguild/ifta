<%- if current_user.role == 'attendee'%>
	<%- itinerary ||= Itinerary.find(session[:itinerary_id])%>
	
	<%= link_to "Registration", edit_itinerary_path(itinerary) %>
	<%= link_to "Proposal Submission", splash_itinerary_proposals_path(itinerary) %>
	<%= link_to "Update Contact Info", edit_user_path(itinerary.user) %>
	<%= link_to "Change Password"  %>
	
<%- elsif current_user.role == 'admin' %>
	<%- conference ||= Conference.active %>
	
	<%= link_to "Dash", conference_path(conference)%>
	<%= link_to "Settings", edit_conference_path(conference)%>
	<%= link_to "Pricing", edit_conference_path(conference, pricing: true)%>
	<%= link_to "Manage IFTA Members", mass_new_members_path %>
	<%= link_to "Manage Attendees", users_path(email: false) %>
<%- end %>

<%= link_to "Log Out", users_sign_out_path %>