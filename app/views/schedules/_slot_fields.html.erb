<b>Slot #<%= f.object.id %></b><br> 
<%= f.input :proposal_id, :label => 'Proposal ID#', :as => 'string' %>

<div class="control-group">
	<%= f.label :room_id, 'Room', :class => "control-label" %>
	<div class="controls">
		<%= f.collection_select(:room_id, @rooms, :id, :label, :prompt => true) %>
	</div>
</div>
<div class="control-group">
	<%= f.label :code, "Code", :class => 'control-label' %>
	<div class="controls">
		<%= f.text_field :code %>
	</div>
</div>
<div class="control-group">
	<%= f.label :comments, "Comments", :class => 'control-label' %>
	<div class="controls">
		<%= f.text_field :comments %>
	</div>
</div>
